mixin installer(item)
    .list
        a(href="/installer/"+item._id)
            img(src=item.resulturl,width="100px").installer__img
            h3=item.title
            a(href="/user/"+item.owner._id)=item.owner.username
            small #{new Date(item.createAt).toLocaleString("ko-kr",{weekday:"long", year:"numeric", month:"2-digit", day:"2-digit"})}
